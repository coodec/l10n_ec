<?xml version="1.0" encoding="UTF-8"?>
<openerp>
  <data>

<record id="view_autorizacion_form" model="ir.ui.view">
  <field name="name">view.autorizacion.form</field>
  <field name="model">l10n_ec_sri_autorizaciones.autorizacion</field>
  <field name="arch" type="xml">
      <form string="Autorizaciones.">
          <sheet>
              <field name="autorizacion_propia" />
              <field name="partner_id" />
              <field name="numero" />
              <field name="establecimiento" />
              <field name="punto_impresion" />
              <field name="valido_desde" />
              <field name="valido_hasta" />
              <group string="Documentos autorizados">
                  <field name="lineaautorizacion_ids" nolabel="1"/>
              </group>
          </sheet>
      </form>
  </field>
</record>

<record id="view_lineaautorizacion_tree" model="ir.ui.view">
    <field name="name">view.lineaautorizacion.tree</field>
    <field name="model">l10n_ec_sri_autorizaciones.lineaautorizacion</field>
    <field name="arch" type="xml">
        <tree>
            <field name="comprobante_id"/>
            <field name="secuencia_inicial"/>
            <field name="secuencia_final"/>
            <field name="secuencia_siguiente"/>
        </tree>
    </field>
</record>


  </data>
</openerp>
